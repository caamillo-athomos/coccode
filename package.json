{
  "name": "coccode",
  "module": "src/index.jsx",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bun run --hot src/index.jsx\" \"bun run tw:dev\"",
    "tw": "bunx tailwindcss -i ./src/style.css -o ./tailwind/output.css",
    "tw:dev": "bunx tailwindcss -i ./src/style.css -o ./tailwind/output.css --watch"
  },
  "devDependencies": {
    "bun-types": "latest",
    "typed-html": "^3.0.1"
  },
  "dependencies": {
    "@elysiajs/html": "^0.6.5",
    "concurrently": "^8.2.1",
    "elysia": "^0.6.23",
    "tailwindcss": "^3.3.3"
  }
}